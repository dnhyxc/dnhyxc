{
  "name": "dnhyxc",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/index.js",
  "homepage": ".",
  "author": "dnhyxc",
  "description": "a web app of electron",
  "scripts": {
    "dev": "chcp 65001 && cross-env NODE_ENV=development vite",
    "build": "rimraf dist release && vue-tsc --noEmit && cross-env NODE_ENV=production && vite build && electron-builder",
    "preview": "vite preview",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier": "prettier --write",
    "commit": "git-cz",
    "prepare": "husky install",
    "test": "npx eslint ./src  --ext ts,vue,js --fix"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run eslint",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "element-plus": "^2.2.28",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "pinia": "^2.0.29",
    "prettier": "^2.8.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "commitizen": "^4.2.6",
    "commitlint-config-cz": "^0.13.3",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "electron": "^22.1.0",
    "electron-builder": "^23.6.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-define-config": "^1.12.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.1.0",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.2",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.0.0",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.11.4",
    "vue-tsc": "^1.0.11"
  },
  "build": {
    "appId": "com.electron.desktop",
    "productName": "dnhyxc",
    "asar": true,
    "copyright": "Copyright © 2022 dnhyxc",
    "directories": {
      "output": "release/"
    },
    "files": [
      "dist",
      "dist-electron"
    ],
    "mac": {
      "icon": "./dist/favicon.ico",
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "./dist/favicon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080"
      }
    ],
    "releaseInfo": {
      "releaseNotes": "版本更新的具体内容"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
